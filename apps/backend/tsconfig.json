{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@db/*": ["../../packages/db/src/*"]
    },
    "outDir": "dist"
  },
  "include": ["src", "."],
  "exclude": ["node_modules"]
}
